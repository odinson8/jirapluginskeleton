<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="com.veniture.resourceManagement-tests" name="resourceManagement" plugins-version="2"> 
  <plugin-info> 
    <description>This is the com.veniture:resourceManagement plugin for Atlassian JIRA.</description>  
    <version>1.0.0</version>  
    <vendor name="venITure" url="http://www.venITure.net/"/> 
  </plugin-info>  
  <!-- from our base plugin -->  
  <component-import key="myComponent" interface="com.veniture.api.MyPluginComponent"/>  
  <!-- from the product container -->  
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>  
  <component key="my-component-wired-test" class="it.com.veniture.MyComponentWiredTest" public="true">
    <interface>it.com.veniture.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
